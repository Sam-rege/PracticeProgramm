class M8  
{
	// design a method to accept 2 values of int and char 
	// print those values 


	public static void demo( int arg1 , char arg2 )
	{
		System.out.println(arg1 + " , " + arg2  ) ;
	}

	public static void main(String[] args) 
	{
		demo(  10 , 'a' )  ;
		demo( 20 , 'b' ) ; 
	}
}
